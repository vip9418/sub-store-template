{
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "https://8.8.8.8/dns-query",
        "detour": "proxy"
      },
      {
        "tag": "tx",
        "address": "https://120.53.53.53/dns-query",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tx",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "tx"
      },
      {
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
        "query_type": ["A", "AAAA"],
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "geosite-cn",
        "server": "tx"
      }
    ],
    "final": "google",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "port": 53,
        "action": "route",
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-cn",
        "action": "route",
        "outbound": "cn"
      },
      {
        "rule_set": ["geoip-netflix", "geosite-netflix"],
        "action": "route",
        "outbound": "auto-us"
      },
      {
        "rule_set": "bahamut",
        "action": "route",
        "outbound": "auto-tw"
      },
      {
        "rule_set": "youtube",
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "rule_set": "openai",
        "action": "route",
        "outbound": "auto-us"
      },
      {
        "rule_set": ["geoip-google", "geosite-google"],
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "rule_set": ["geoip-apple", "geosite-apple"],
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "rule_set": ["geoip-telegram", "geosite-telegram"],
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "rule_set": "geolocation-!cn",
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "protocol": ["http", "https", "tls", "quic", "grpc"],
        "action": "route",
        "outbound": "final"
      },
      {
        "domain": ["google.com", "gstatic.com"],
        "action": "route",
        "outbound": "auto-all"
      },
      {
        "outbound": "final",
        "action": "route"
      }
    ],
    "rule_set": [
      {
        "tag": "geoip-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geoip/apple.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/apple.srs",
        "download_detour": "direct"
      },
      {
        "tag": "bahamut",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/bahamut.srs",
        "download_detour": "direct"
      },
      {
        "tag": "bilibili",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/bilibili.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "direct"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "dns-out",
      "type": "dns"
    },
    {
      "tag": "proxy",
      "type": "urltest",
      "outbounds": ["auto-all"],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "5m"
    },
    {
      "tag": "auto-all",
      "type": "urltest",
      "outbounds": [],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "5m",
      "tolerance": 150
    },
    {
