      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": [
          "geoip-netflix",
          "geosite-netflix"
        ],
        "outbound": "netflix"
      },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google"
        ],
        "outbound": "google"
      },
      {
        "rule_set": [
          "geoip-apple",
          "geosite-apple"
        ],
        "outbound": "apple"
      },
      {
        "rule_set": "geolocation-!cn",
        "outbound": "proxy"
