{
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "oversea-dns",
        "address": "tls://8.8.4.4",
        "detour": "proxy"
      },
      {
        "tag": "cn-dns",
        "address": "https://doh.pub/dns-query",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "domain_suffix": ["cn"],
        "server": "cn-dns"
      },
      {
        "domain_keyword": ["gov.cn", "edu.cn"],
        "server": "cn-dns"
      },
      {
        "rule_set": "geosite-cn",
        "server": "cn-dns"
      },
      {
        "query_type": ["A", "AAAA"],
        "server": "fakeip"
      },
      {
        "outbound": "any",
        "server": "cn-dns",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "cn-dns"
      },
      {
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite-cn",
        "server": "cn-dns"
      }
    ],
    "final": "oversea-dns",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geoip-cn",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-cn",
