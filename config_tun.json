{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "proxy",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db",
      "store_fakeip": false
    }
  },
  
  "dns": {
    "servers": [
      {"tag": "proxyDns", "address": "https://dns.google/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "proxy"},
      {"tag": "localDns", "address": "https://dns.alidns.com/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "direct"},
      {"tag": "defaultDns", "address": "https://dns.alidns.com/dns-query", "address_strategy": "ipv4_only", "detour": "direct"},
      {"tag": "cloudflareDns", "address": "https://1.1.1.1/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "proxy"},
      {"tag": "quad9Dns", "address": "https://dns.quad9.net/dns-query", "address_resolver": "defaultDns", "address_strategy": "ipv4_only", "detour": "proxy"}
    ],
    "rules": [
      { "outbound": "any", "server": "defaultDns", "disable_cache": true },
      { "clash_mode": "direct", "server": "localDns" },
      { "clash_mode": "global", "server": "proxyDns" },
      { "rule_set": "geosite-cn", "server": "localDns" },
      { "rule_set": "geosite-geolocation-!cn", "server": "proxyDns" }
    ],
    "final": "localDns",
    "strategy": "ipv4_only"
  },

  "inbounds": [
    {
      "type": "tun",
      "address": "172.19.0.1/30",
      "mtu": 9000,
      "auto_route": true,
      "sniff": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],

  "outbounds": [
    { "tag": "proxy", "type": "selector", "outbounds": ["auto-hk", "hk", "jp", "us", "manual", "auto", "direct"] },
    { "tag": "youtube", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "openai", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "google", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "github", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "microsoft", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "direct"] },
    { "tag": "onedrive", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "tiktok", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "netflix", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "telegram", "type": "selector", "outbounds": ["proxy", "auto", "auto-hk", "hk", "jp", "us", "manual"] },
    { "tag": "apple", "type": "selector", "outbounds": ["direct", "hk", "jp", "us"] },
    { "tag": "final", "type": "selector", "outbounds": ["proxy", "direct"] },
    { "tag": "manual", "type": "selector", "outbounds": ["proxy", "direct", "hk", "jp", "us"] },
    { "tag": "hk", "type": "selector", "outbounds": ["proxy", "direct", "jp", "us"], "filter": [{ "action": "include", "keywords": ["hk|HongKong"] }] },
    { "tag": "jp", "type": "selector", "outbounds": ["proxy", "direct", "hk", "us"], "filter": [{ "action": "include", "keywords": ["jp|Japan"] }] },
    { "tag": "us", "type": "selector", "outbounds": ["proxy", "direct", "hk", "jp"], "filter": [{ "action": "include", "keywords": ["us|United States"] }, { "action": "exclude", "keywords": ["hk|HongKong"] }] },
    { "tag": "auto-hk", "type": "urltest", "outbounds": ["hk"], "url": "http://www.gstatic.com/generate_204", "interval": "10m", "tolerance": 50 },
    { "tag": "auto", "type": "urltest", "outbounds": ["proxy", "direct", "hk", "jp", "us"], "url": "http://www.gstatic.com/generate_204", "interval": "10m", "tolerance": 50 },
    { "tag": "direct", "type": "direct" },
    { "tag": "dns-out", "type": "dns" }
  ],
  
  "route": {
    "auto_detect_interface": true,
    "final": "final",
    "rules": [
      { "type": "logical", "mode": "or", "rules": [{ "port": 53 }, { "protocol": "dns" }], "outbound": "dns-out" },
      { "clash_mode": "direct", "outbound": "direct" },
      { "clash_mode": "global", "outbound": "proxy" },
      { "domain": ["clash.razord.top", "yacd.metacubex.one", "yacd.haishan.me", "d.metacubex.one"], "outbound": "direct" },
      { "rule_set": ["geosite-private", "geoip-private"], "outbound": "direct" },
      { "rule_set": "geosite-chat", "outbound": "openai" },
      { "rule_set": "geosite-github", "outbound": "github" },
      { "rule_set": "geosite-youtube", "outbound": "youtube" },
      { "rule_set": "geosite-onedrive", "outbound": "onedrive" }, 
      { "rule_set": "geosite-microsoft", "outbound": "microsoft" }, 
      { "rule_set": ["geoip-google", "geosite-google"], "outbound": "google" },
      { "rule_set": "geosite-tiktok", "outbound": "tiktok" },
      { "rule_set": ["geoip-apple", "geosite-apple"], "outbound": "apple" },
      { "rule_set": ["geoip-netflix", "geosite-netflix"], "outbound": "netflix" },
      { "rule_set": ["geoip-telegram", "geosite-telegram"], "outbound": "telegram" },
      { "rule_set": "geosite-geolocation-!cn", "outbound": "proxy" },
      { "rule_set": ["geoip-cn", "geosite-cn"], "outbound": "direct" },
      { "rule_set": "geosite-europe", "outbound": "proxy" },
      { "rule_set": "geosite-singapore", "outbound": "proxy" },
      { "rule_set": "geosite-korea", "outbound": "proxy" },
      { "rule_set": "geosite-australia", "outbound": "proxy" },
      { "rule_set": "geosite-canada", "outbound": "proxy" }
    ],
    
    "rule_set": [
      { "tag": "geosite-chat", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-chat-!cn.srs", "download_detour": "proxy" },
      { "tag": "geosite-europe", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/europe.srs", "download_detour": "proxy" },
      { "tag": "geosite-singapore", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/singapore.srs", "download_detour": "proxy" },
      { "tag": "geosite-korea", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/korea.srs", "download_detour": "proxy" },
      { "tag": "geosite-australia", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/australia.srs", "download_detour": "proxy" },
      { "tag": "geosite-canada", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/canada.srs", "download_detour": "proxy" },
      { "tag": "geosite-youtube", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs", "download_detour": "proxy" },
      { "tag": "geosite-google", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs", "download_detour": "proxy" },
      { "tag": "geosite-github", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/github.srs", "download_detour": "proxy" },
      { "tag": "geosite-telegram", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs", "download_detour": "proxy" },
      { "tag": "geosite-tiktok", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/tiktok.srs", "download_detour": "proxy" },
      { "tag": "geosite-netflix", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/netflix.srs", "download_detour": "proxy" },
      { "tag": "geosite-apple", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/apple.srs", "download_detour": "proxy" },
      { "tag": "geosite-microsoft", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft.srs", "download_detour": "proxy" },
      { "tag": "geosite-onedrive", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/onedrive.srs", "download_detour": "proxy" },
      { "tag": "geosite-geolocation-!cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/geolocation-!cn.srs", "download_detour": "proxy" },
      { "tag": "geosite-cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs", "download_detour": "proxy" },
      { "tag": "geosite-private", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/private.srs", "download_detour": "proxy" },
            
      { "tag": "geoip-google", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs", "download_detour": "🚀 节点选择" },
      { "tag": "geoip-telegram", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs", "download_detour": "🚀 节点选择" },     
      { "tag": "geoip-netflix", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/netflix.srs", "download_detour": "🚀 节点选择" },     
      { "tag": "geoip-apple", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo-lite/geoip/apple.srs", "download_detour": "🚀 节点选择" },
      { "tag": "geoip-cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs", "download_detour": "🚀 节点选择" },
      { "tag": "geoip-private", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/private.srs", "download_detour": "🚀 节点选择" }
    ]
  }
}
